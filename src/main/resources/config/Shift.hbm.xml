<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="GaiaProjectAppService.models.entities.Shift" table="Shift" schema="GaiaDatabase">
        <id name="idShift">
            <column name="IdShift" sql-type="int(11)"/>
        </id>
        <property name="initialDate">
            <column name="initialDate" sql-type="date" not-null="true"/>
        </property>
        <property name="finalDate">
            <column name="finalDate" sql-type="date" not-null="true"/>
        </property>
        <property name="shiftName">
            <column name="shiftName" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="hour">
            <column name="hour" sql-type="time" not-null="true"/>
        </property>
        <set name="botanicsByIdShift" inverse="true">
            <key>
                <column name="idShift" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.Botanic"/>
        </set>
        <set name="caretakersByIdShift" inverse="true">
            <key>
                <column name="idShift" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.Caretaker"/>
        </set>
        <set name="guidesByIdShift" inverse="true">
            <key>
                <column name="idShift" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.Guide"/>
        </set>
        <set name="managementsByIdShift" inverse="true">
            <key>
                <column name="idShift" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.Management"/>
        </set>
        <set name="veterinariesByIdShift" inverse="true">
            <key>
                <column name="idShift" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.Veterinary"/>
        </set>
    </class>
</hibernate-mapping>