<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="GaiaProjectAppService.models.entities.Itinerary" table="Itinerary" schema="GaiaDatabase">
        <id name="idItinerary">
            <column name="idItinerary" sql-type="int(11)"/>
        </id>
        <property name="itineraryCode">
            <column name="itineraryCode" sql-type="varchar(12)" length="12"/>
        </property>
        <property name="itineraryDate">
            <column name="itineraryDate" sql-type="date"/>
        </property>
        <property name="walkDuration">
            <column name="walkDuration" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="itineraryLength">
            <column name="itineraryLength" sql-type="double" precision="-1"/>
        </property>
        <property name="maxNumberPeople">
            <column name="maxNumberPeople" sql-type="int(11)"/>
        </property>
        <property name="maxNumberVisitedSpecie">
            <column name="maxNumberVisitedSpecie" sql-type="int(11)"/>
        </property>
        <set name="itineraryGuidesByIdItinerary" inverse="true">
            <key>
                <column name="idItinerary"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.ItineraryGuide"/>
        </set>
        <set name="itineraryZonesByIdItinerary" inverse="true">
            <key>
                <column name="idItinerary"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.ItineraryZone"/>
        </set>
    </class>
</hibernate-mapping>