<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="GaiaProjectAppService.models.entities.ItineraryZone" table="Itinerary_Zone" schema="GaiaDatabase">
        <id name="idItineraryZone">
            <column name="idItineraryZone" sql-type="int(11)"/>
        </id>
        <property name="idZone">
            <column name="idZone" sql-type="int(11)"/>
        </property>
        <property name="idItinerary">
            <column name="idItinerary" sql-type="int(11)"/>
        </property>
        <many-to-one name="zoneByIdZone" class="GaiaProjectAppService.models.entities.Zone" insert="false" update="false">
            <column name="idZone"/>
        </many-to-one>
        <many-to-one name="itineraryByIdItinerary" class="GaiaProjectAppService.models.entities.Itinerary" insert="false" update="false">
            <column name="idItinerary"/>
        </many-to-one>
    </class>
</hibernate-mapping>