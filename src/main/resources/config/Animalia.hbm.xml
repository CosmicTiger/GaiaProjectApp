<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="GaiaProjectAppService.models.entities.Animalia" table="Animalia" schema="GaiaDatabase">
        <id name="idAnimalia">
            <column name="idAnimalia" sql-type="int(11)"/>
        </id>
        <property name="idSpecie">
            <column name="idSpecie" sql-type="int(11)"/>
        </property>
        <property name="description">
            <column name="description" sql-type="varchar(90)" length="90" not-null="true"/>
        </property>
        <property name="origin">
            <column name="origin" sql-type="varchar(60)" length="60"/>
        </property>
        <property name="status">
            <column name="status" sql-type="varchar(50)" length="50"/>
        </property>
        <many-to-one name="specieByIdSpecie" class="GaiaProjectAppService.models.entities.Specie" insert="false" update="false">
            <column name="idSpecie"/>
        </many-to-one>
        <set name="animaliaZonesByIdAnimalia" inverse="true">
            <key>
                <column name="idAnimalia"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.AnimaliaZone"/>
        </set>
        <set name="caretakerAnimaliasByIdAnimalia" inverse="true">
            <key>
                <column name="idAnimalia"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.CaretakerAnimalia"/>
        </set>
        <set name="transferencesAnimaliasByIdAnimalia" inverse="true">
            <key>
                <column name="idAnimalia"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.TransferencesAnimalia"/>
        </set>
        <set name="veterinaryAnimaliasByIdAnimalia" inverse="true">
            <key>
                <column name="idAnimalia"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.VeterinaryAnimalia"/>
        </set>
    </class>
</hibernate-mapping>