<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="GaiaProjectAppService.models.entities.Plantae" table="Plantae" schema="GaiaDatabase">
        <id name="idPlantae">
            <column name="idPlantae" sql-type="int(11)"/>
        </id>
        <property name="idSpecie">
            <column name="idSpecie" sql-type="int(11)"/>
        </property>
        <property name="description">
            <column name="description" sql-type="varchar(90)" length="90" not-null="true"/>
        </property>
        <property name="origin">
            <column name="origin" sql-type="varchar(60)" length="60"/>
        </property>
        <property name="status">
            <column name="status" sql-type="varchar(50)" length="50"/>
        </property>
        <many-to-one name="specieByIdSpecie" class="GaiaProjectAppService.models.entities.Specie" insert="false" update="false">
            <column name="idSpecie"/>
        </many-to-one>
        <set name="botanicPlantaesByIdPlantae" inverse="true">
            <key>
                <column name="idPlantae"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.BotanicPlantae"/>
        </set>
        <set name="caretakerPlantaesByIdPlantae" inverse="true">
            <key>
                <column name="idPlantae"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.CaretakerPlantae"/>
        </set>
        <set name="plantaeZonesByIdPlantae" inverse="true">
            <key>
                <column name="idPlantae"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.PlantaeZone"/>
        </set>
        <set name="transferencesPlantaesByIdPlantae" inverse="true">
            <key>
                <column name="idPlantae"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.TransferencesPlantae"/>
        </set>
    </class>
</hibernate-mapping>