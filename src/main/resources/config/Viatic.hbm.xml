<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="GaiaProjectAppService.models.entities.Viatic" table="Viatic" schema="GaiaDatabase">
        <id name="idViatic">
            <column name="idViatic" sql-type="int(11)"/>
        </id>
        <property name="idStaff">
            <column name="idStaff" sql-type="int(11)"/>
        </property>
        <property name="viaticCode">
            <column name="viaticCode" sql-type="varchar(12)" length="12"/>
        </property>
        <property name="concept">
            <column name="concept" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="dateViatic">
            <column name="dateViatic" sql-type="date"/>
        </property>
        <property name="amount">
            <column name="amount" sql-type="double" precision="-1"/>
        </property>
        <many-to-one name="staffByIdStaff" class="GaiaProjectAppService.models.entities.Staff" insert="false" update="false">
            <column name="idStaff"/>
        </many-to-one>
    </class>
</hibernate-mapping>