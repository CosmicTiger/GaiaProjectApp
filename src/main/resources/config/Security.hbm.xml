<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="GaiaProjectAppService.models.entities.Security" table="Security" schema="GaiaDatabase">
        <id name="idSecurity">
            <column name="idSecurity" sql-type="int(11)"/>
        </id>
        <property name="idStaff">
            <column name="idStaff" sql-type="int(11)"/>
        </property>
        <property name="idZone">
            <column name="idZone" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="role">
            <column name="role" sql-type="varchar(60)" length="60" not-null="true"/>
        </property>
        <property name="descriptionRole">
            <column name="descriptionRole" sql-type="varchar(70)" length="70" not-null="true"/>
        </property>
        <property name="idShift">
            <column name="idShift" sql-type="int(11)" not-null="true"/>
        </property>
        <many-to-one name="staffByIdStaff" class="GaiaProjectAppService.models.entities.Staff" insert="false" update="false">
            <column name="idStaff"/>
        </many-to-one>
        <many-to-one name="zoneByIdZone" class="GaiaProjectAppService.models.entities.Zone" insert="false" update="false">
            <column name="idZone" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>