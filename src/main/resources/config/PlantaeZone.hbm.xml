<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="GaiaProjectAppService.models.entities.PlantaeZone" table="Plantae_Zone" schema="GaiaDatabase">
        <id name="idPlantaeZone">
            <column name="idPlantaeZone" sql-type="int(11)"/>
        </id>
        <property name="idPlantae">
            <column name="idPlantae" sql-type="int(11)"/>
        </property>
        <property name="idZone">
            <column name="idZone" sql-type="int(11)"/>
        </property>
        <many-to-one name="plantaeByIdPlantae" class="GaiaProjectAppService.models.entities.Plantae" insert="false" update="false">
            <column name="idPlantae"/>
        </many-to-one>
        <many-to-one name="zoneByIdZone" class="GaiaProjectAppService.models.entities.Zone" insert="false" update="false">
            <column name="idZone"/>
        </many-to-one>
    </class>
</hibernate-mapping>