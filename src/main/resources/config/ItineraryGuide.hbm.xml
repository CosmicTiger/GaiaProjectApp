<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="GaiaProjectAppService.models.entities.ItineraryGuide" table="ItineraryGuide" schema="GaiaDatabase">
        <id name="idItineraryGuide">
            <column name="idItineraryGuide" sql-type="int(11)"/>
        </id>
        <property name="idGuide">
            <column name="idGuide" sql-type="int(11)"/>
        </property>
        <property name="idItinerary">
            <column name="idItinerary" sql-type="int(11)"/>
        </property>
        <many-to-one name="guideByIdGuide" class="GaiaProjectAppService.models.entities.Guide" insert="false" update="false">
            <column name="idGuide"/>
        </many-to-one>
        <many-to-one name="itineraryByIdItinerary" class="GaiaProjectAppService.models.entities.Itinerary" insert="false" update="false">
            <column name="idItinerary"/>
        </many-to-one>
    </class>
</hibernate-mapping>