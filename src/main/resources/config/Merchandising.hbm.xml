<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="GaiaProjectAppService.models.entities.Merchandising" table="Merchandising" schema="GaiaDatabase">
        <id name="idMerchandising">
            <column name="idMerchandising" sql-type="int(11)"/>
        </id>
        <property name="idWarehouse">
            <column name="idWarehouse" sql-type="int(11)"/>
        </property>
        <property name="idMerchandisingZone">
            <column name="idMerchandisingZone" sql-type="int(11)"/>
        </property>
        <property name="admissionDate">
            <column name="admissionDate" sql-type="date"/>
        </property>
        <property name="expirationDate">
            <column name="expirationDate" sql-type="date"/>
        </property>
        <many-to-one name="warehouseByIdWarehouse" class="GaiaProjectAppService.models.entities.Warehouse" insert="false" update="false">
            <column name="idWarehouse"/>
        </many-to-one>
        <many-to-one name="zoneByIdMerchandisingZone" class="GaiaProjectAppService.models.entities.Zone" insert="false" update="false">
            <column name="idMerchandisingZone"/>
        </many-to-one>
        <set name="saleDetailsByIdMerchandising" inverse="true">
            <key>
                <column name="idMerchandising"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.SaleDetail"/>
        </set>
    </class>
</hibernate-mapping>