<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="GaiaProjectAppService.models.entities.MonetaryDonation" table="Monetary_Donation" schema="GaiaDatabase">
        <id name="idMonetaryDonation">
            <column name="idMonetaryDonation" sql-type="int(11)"/>
        </id>
        <property name="idDonation">
            <column name="idDonation" sql-type="int(11)"/>
        </property>
        <property name="amountMoney">
            <column name="amountMoney" sql-type="double" precision="-1"/>
        </property>
        <many-to-one name="donationByIdDonation" class="GaiaProjectAppService.models.entities.Donation" insert="false" update="false">
            <column name="idDonation"/>
        </many-to-one>
    </class>
</hibernate-mapping>