<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="GaiaProjectAppService.models.entities.TransferencesAnimalia" table="Transferences_Animalia"
           schema="GaiaDatabase">
        <id name="idTransferenceAnimalia">
            <column name="idTransferenceAnimalia" sql-type="int(11)"/>
        </id>
        <property name="idAnimalia">
            <column name="idAnimalia" sql-type="int(11)"/>
        </property>
        <property name="idTransferences">
            <column name="idTransferences" sql-type="int(11)"/>
        </property>
        <many-to-one name="animaliaByIdAnimalia" class="GaiaProjectAppService.models.entities.Animalia" insert="false" update="false">
            <column name="idAnimalia"/>
        </many-to-one>
        <many-to-one name="transferencesByIdTransferences" class="GaiaProjectAppService.models.entities.Transferences" insert="false" update="false">
            <column name="idTransferences"/>
        </many-to-one>
    </class>
</hibernate-mapping>