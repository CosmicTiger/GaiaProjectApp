<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="GaiaProjectAppService.models.entities.InventoryDonation" table="Inventory_Donation" schema="GaiaDatabase">
        <id name="idInventoryDonation">
            <column name="idInventoryDonation" sql-type="int(11)"/>
        </id>
        <property name="idSupply">
            <column name="idSupply" sql-type="int(11)"/>
        </property>
        <property name="idDonation">
            <column name="idDonation" sql-type="int(11)"/>
        </property>
        <many-to-one name="supplyByIdSupply" class="GaiaProjectAppService.models.entities.Supply" insert="false" update="false">
            <column name="idSupply"/>
        </many-to-one>
        <many-to-one name="donationByIdDonation" class="GaiaProjectAppService.models.entities.Donation" insert="false" update="false">
            <column name="idDonation"/>
        </many-to-one>
    </class>
</hibernate-mapping>