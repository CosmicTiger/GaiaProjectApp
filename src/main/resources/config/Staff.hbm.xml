<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="GaiaProjectAppService.models.entities.Staff" table="Staff" schema="GaiaDatabase">
        <id name="idStaff">
            <column name="idStaff" sql-type="int(11)"/>
        </id>
        <property name="staffCode">
            <column name="staffCode" sql-type="varchar(12)" length="12"/>
        </property>
        <property name="firstName">
            <column name="firstName" sql-type="varchar(25)" length="25"/>
        </property>
        <property name="middleName">
            <column name="middleName" sql-type="varchar(25)" length="25" not-null="true"/>
        </property>
        <property name="lastNamePrimary">
            <column name="lastNamePrimary" sql-type="varchar(25)" length="25"/>
        </property>
        <property name="lastNameSecondary">
            <column name="lastNameSecondary" sql-type="varchar(25)" length="25" not-null="true"/>
        </property>
        <property name="telephone">
            <column name="telephone" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="address">
            <column name="address" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(25)" length="25"/>
        </property>
        <property name="salary">
            <column name="salary" sql-type="double" precision="-1"/>
        </property>
        <set name="botanicsByIdStaff" inverse="true">
            <key>
                <column name="idStaff"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.Botanic"/>
        </set>
        <set name="caretakersByIdStaff" inverse="true">
            <key>
                <column name="idStaff"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.Caretaker"/>
        </set>
        <set name="guidesByIdStaff" inverse="true">
            <key>
                <column name="idStaff"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.Guide"/>
        </set>
        <set name="managementsByIdStaff" inverse="true">
            <key>
                <column name="idStaff"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.Management"/>
        </set>
        <set name="paymentDetailsByIdStaff" inverse="true">
            <key>
                <column name="idStaff"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.PaymentDetail"/>
        </set>
        <set name="securitiesByIdStaff" inverse="true">
            <key>
                <column name="idStaff"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.Security"/>
        </set>
        <set name="usersByIdStaff" inverse="true">
            <key>
                <column name="idStaff"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.User"/>
        </set>
        <set name="veterinariesByIdStaff" inverse="true">
            <key>
                <column name="idStaff"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.Veterinary"/>
        </set>
        <set name="viaticsByIdStaff" inverse="true">
            <key>
                <column name="idStaff"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.Viatic"/>
        </set>
    </class>
</hibernate-mapping>