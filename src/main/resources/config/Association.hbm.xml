<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="GaiaProjectAppService.models.entities.Association" table="Association" schema="GaiaDatabase">
        <id name="idAssociation">
            <column name="idAssociation" sql-type="int(11)"/>
        </id>
        <property name="associationCode">
            <column name="associationCode" sql-type="varchar(12)" length="12"/>
        </property>
        <property name="associationName">
            <column name="associationName" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="concept">
            <column name="concept" sql-type="varchar(70)" length="70"/>
        </property>
        <property name="associationType">
            <column name="associationType" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="objective">
            <column name="objective" sql-type="varchar(200)" length="200"/>
        </property>
        <property name="telephone">
            <column name="telephone" sql-type="varchar(25)" length="25"/>
        </property>
        <property name="address">
            <column name="address" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="country">
            <column name="country" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="province">
            <column name="province" sql-type="varchar(30)" length="30"/>
        </property>
        <set name="donationsByIdAssociation" inverse="true">
            <key>
                <column name="idAssociation"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.Donation"/>
        </set>
    </class>
</hibernate-mapping>