<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="GaiaProjectAppService.models.entities.Botanic" table="Botanic" schema="GaiaDatabase">
        <id name="idBotanic">
            <column name="idBotanic" sql-type="int(11)"/>
        </id>
        <property name="idStaff">
            <column name="idStaff" sql-type="int(11)"/>
        </property>
        <property name="role">
            <column name="role" sql-type="varchar(60)" length="60" not-null="true"/>
        </property>
        <property name="descriptionRole">
            <column name="descriptionRole" sql-type="varchar(70)" length="70" not-null="true"/>
        </property>
        <property name="idShift">
            <column name="idShift" sql-type="int(11)" not-null="true"/>
        </property>
        <many-to-one name="staffByIdStaff" class="GaiaProjectAppService.models.entities.Staff" insert="false" update="false">
            <column name="idStaff"/>
        </many-to-one>
        <many-to-one name="shiftByIdShift" class="GaiaProjectAppService.models.entities.Shift" insert="false" update="false">
            <column name="idShift" not-null="true"/>
        </many-to-one>
        <set name="botanicPlantaesByIdBotanic" inverse="true">
            <key>
                <column name="idBotanic"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.BotanicPlantae"/>
        </set>
    </class>
</hibernate-mapping>