<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="GaiaProjectAppService.models.entities.Transferences" table="Transferences" schema="GaiaDatabase">
        <id name="idTransferences">
            <column name="idTransferences" sql-type="int(11)"/>
        </id>
        <property name="idProgram">
            <column name="idProgram" sql-type="int(11)"/>
        </property>
        <property name="idSpecie">
            <column name="idSpecie" sql-type="int(11)"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(70)" length="70"/>
        </property>
        <property name="transferenceCode">
            <column name="transferenceCode" sql-type="varchar(12)" length="12"/>
        </property>
        <property name="dateTransference">
            <column name="dateTransference" sql-type="date"/>
        </property>
        <property name="age">
            <column name="age" sql-type="int(11)"/>
        </property>
        <many-to-one name="programByIdProgram" class="GaiaProjectAppService.models.entities.Program" insert="false" update="false">
            <column name="idProgram"/>
        </many-to-one>
        <many-to-one name="specieByIdSpecie" class="GaiaProjectAppService.models.entities.Specie" insert="false" update="false">
            <column name="idSpecie"/>
        </many-to-one>
        <set name="transferencesAnimaliasByIdTransferences" inverse="true">
            <key>
                <column name="idTransferences"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.TransferencesAnimalia"/>
        </set>
        <set name="transferencesPlantaesByIdTransferences" inverse="true">
            <key>
                <column name="idTransferences"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.TransferencesPlantae"/>
        </set>
    </class>
</hibernate-mapping>