<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="GaiaProjectAppService.models.entities.Zone" table="Zone" schema="GaiaDatabase">
        <id name="idZone">
            <column name="idZone" sql-type="int(11)"/>
        </id>
        <property name="zoneCode">
            <column name="zoneCode" sql-type="varchar(12)" length="12"/>
        </property>
        <property name="nameZone">
            <column name="nameZone" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="extension">
            <column name="extension" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="description">
            <column name="description" sql-type="varchar(70)" length="70"/>
        </property>
        <property name="wheater">
            <column name="wheater" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="role">
            <column name="role" sql-type="varchar(50)" length="50"/>
        </property>
        <set name="animaliaZonesByIdZone" inverse="true">
            <key>
                <column name="idZone"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.AnimaliaZone"/>
        </set>
        <set name="itineraryZonesByIdZone" inverse="true">
            <key>
                <column name="idZone"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.ItineraryZone"/>
        </set>
        <set name="merchandisingsByIdZone" inverse="true">
            <key>
                <column name="idMerchandisingZone"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.Merchandising"/>
        </set>
        <set name="plantaeZonesByIdZone" inverse="true">
            <key>
                <column name="idZone"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.PlantaeZone"/>
        </set>
        <set name="securitiesByIdZone" inverse="true">
            <key>
                <column name="idZone" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.Security"/>
        </set>
        <set name="warehousesByIdZone" inverse="true">
            <key>
                <column name="idZone"/>
            </key>
            <one-to-many not-found="ignore" class="GaiaProjectAppService.models.entities.Warehouse"/>
        </set>
    </class>
</hibernate-mapping>